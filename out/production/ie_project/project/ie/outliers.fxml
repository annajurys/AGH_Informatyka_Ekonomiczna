<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #011627;  -fx-background-radius: 0;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.ie.OutliersController">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Label fx:id="labelOutliers" text="Outliers" textFill="#efffe9">
                    <VBox.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Bookman Old Style" size="18.0" />
                    </font></Label>
                <Label fx:id="labelOutlier" alignment="TOP_LEFT" prefHeight="100.0" prefWidth="350.0" style="-fx-background-color: #d3d3d3;" textFill="RED">
                    <VBox.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Bookman Old Style" size="12.0" />
                    </font>
                </Label>
                <Button fx:id="buttonFixTheOutliers" mnemonicParsing="false" onMouseClicked="#onMouseClickedFixTheOutliers" style="-fx-background-color: #EFFFE9;  -fx-background-radius: 0;" text="Fix the outliers">
                    <VBox.margin>
                        <Insets bottom="3.0" left="3.0" right="3.0" top="30.0" />
                    </VBox.margin>
                    <font>
                        <Font name="Bookman Old Style" size="18.0" />
                    </font>
                </Button>
            </children></VBox>
    </children>
</AnchorPane>
